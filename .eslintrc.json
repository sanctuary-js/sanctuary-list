{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es6.json"],
  "overrides": [
    {
      "files": ["*.md"],
      "globals": {"$": "readonly", "Cons": "readonly", "List": "readonly", "Nil": "readonly", "S": "readonly", "Z": "readonly"}
    },
    {
      "files": ["index.js"],
      "globals": {"Deno": "readonly"},
      "rules": {
        "no-unused-vars": ["error", {"vars": "all", "varsIgnorePattern": "^S$", "args": "none"}]
      }
    }
  ]
}
